{
    "id" : "com.mastropaolo.www.tabela",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "47",
    "sdk" : "org.gnome.Sdk",
    "sdk-extensions" : [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command" : "tabela",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland"
    ],
    "build-options" : {
        "append-path" : "/usr/lib/sdk/rust-stable/bin",
        "build-args" : [
        ],
        "env" : {
            "RUST_BACKTRACE" : "1",
            "RUST_LOG" : "tabela=debug"
        }
    },
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "tabela",
            "builddir" : true,
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "archive",
                    "url" : "https://github.com/xanathar/tabela/releases/download/v0.2.1/tabela-0.2.1.tar.xz",
                    "sha256" : "db6235d24f041acbbd5cfd50f17af6f0493d8d04f211b1753db8f643cbe2e48b"
                }
            ],
            "config-opts" : [
                "--buildtype",
                "release"
            ]
        }
    ]
}
